{
  "name": "dashbot",
  "version": "0.8.2",
  "description": "Analytics for your bot",
  "main": "src/dashbot.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actionably/dashbot.git"
  },
  "scripts": {
    "facebook": "nodemon -r dotenv/config src/facebook-example.js",
    "facebook-repeat": "nodemon -r dotenv/config src/facebook-example-with-delay.js",
    "generic": "nodemon -r dotenv/config src/generic-example.js",
    "kik": "nodemon -r dotenv/config src/kik-example-npm.js",
    "slack": "nodemon -r dotenv/config src/slack-example.js",
    "slack-repeat": "REPEAT=true nodemon -r dotenv/config src/slack-example-rest.js",
    "microsoft": "nodemon -r dotenv/config src/microsoft-botframework-example.js",
    "google": "nodemon -r dotenv/config src/google-example-api-ai.js"
  },
  "keywords": [
    "bot",
    "bots",
    "chatbot",
    "facebook messenger",
    "microsoft bot framework",
    "kik",
    "slack",
    "analytics",
    "data",
    "tools"
  ],
  "author": "Jesse Hull",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/actionably/dashbot/issues"
  },
  "homepage": "https://github.com/actionably/dashbot#readme",
  "devDependencies": {
    "actions-on-google": "1.0.1",
    "body-parser": "1.15.0",
    "botbuilder": "3.4.4",
    "botkit": "0.2.2",
    "circular-json": "0.3.0",
    "dotenv": "2.0.0",
    "express": "4.13.4",
    "nodemon": "1.11.0",
    "websocket": "1.0.23"
  },
  "dependencies": {
    "lodash": "4.12.0",
    "node-uuid": "1.4.7",
    "request": "2.72.0",
    "request-promise": "3.0.0"
  }
}
